#include <iostream>
#include <string>
#include <cmath>

using namespace std;

  
double getValue(double variable, string variableName){
	cout << "Введите значение для " << variableName << ": "	;
	while (!(cin >> variable)) {
		cout << "Введите коректное значение!\n";
		cin.clear();
		cin.ignore(256, '\n');
	}
	return variable;
}

int main()
{
	int n, i;
	cout << "Введите число n: ";
	cin >> n;
	i = n;
	for (n = i + 1; n <= i + 10; n++){
		cout << " " << n;
	}
	
}
