#include <iostream>
#include <string>
#include <cmath>

using namespace std;

int main()
{
    float a, b, c, d, x1, x2;
    cout << "a = ";
    cin >> a;
    cout << "b = ";
    cin >> b;
    cout << "c = ";
    cin >> c;
    d = b * b - 4 * a * c;
    if(d >= 0){
        x1 = (-1 * b + sqrt(d)) / 2 * a;
        x2 = (-1 * b - sqrt(d)) / 2 * a;
        }
    cout << "x1 = " << x1 <<"\nx2 = " << x2;
    
}